		<!-- APGAR Scoring -->

        <record model="ir.ui.view" id="medical_neonatal_apgar_view">
            <field name="name">APGAR</field>
			<field name="model">medical.neonatal.apgar</field>
            <field name="type">form</field>
            <field name="arch" type="xml">                
                <form string="APGAR Score" version="7.0">
					<sheet>
                    	<group>
							<field name="apgar_minute"/>                        
							<field name="apgar_score"/>
						</group>
						<group>
							<group>                        
								<field name="apgar_appearance"/>							
								<field name="apgar_pulse"/>
								<field name="apgar_respiration"/>
							</group>
							<group>                        
								<field name="apgar_grimace"/>                       
								<field name="apgar_activity"/>
							</group>
						</group>
					</sheet>
                </form>                
            </field>
        </record>

        <record model="ir.ui.view" id="medical_neonatal_apgar_tree">
			<field name="name">APGAR</field>
            <field name="model">medical.neonatal.apgar</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="APGAR Score">
                    <field name="apgar_minute"/>
                    <field name="apgar_score"/>
                    <field name="apgar_appearance"/>
                    <field name="apgar_pulse"/>
                    <field name="apgar_grimace"/>
                    <field name="apgar_activity"/>
                    <field name="apgar_respiration"/>
                </tree>                
            </field>
        </record>
		
		<!-- Newborn -->
		
		<record model="ir.ui.view" id="medical_patient_newborn_view">
            <field name="name">Newborn</field>
			<field name="model">medical.newborn</field>
            <field name="type">form</field>
            <field name="arch" type="xml">                
                <form string="Newborn Info" version="7.0">
					<header>                    
						<button name="print_card" string="Print Newborn Card" type="object" class="oe_highlight"/>                    
                    </header>
					<sheet>
						<field name="photo"  nolabel="1" class="oe_left oe_avatar" img_width="120" img_height="120" width="120" height="120" widget="image"/>
						<div class="oe_title">
							<div class="oe_edit_only">
								<label for="name"/> 
							</div>
						<h1>
							<field name="name" default_focus="1" placeholder="Newborn ID" />
						</h1>                                                
						</div>                    	
						<group string="Main">
							<group>
								<group>
									<field name="newborn_name"/>	
									<field name="sex"/>			
									<field name="dismissed"/>		
